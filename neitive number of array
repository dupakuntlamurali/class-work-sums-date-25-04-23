#include<stdio.h>
int main()
{
    int n,i;
    printf("enter the size of arrary:");
    scanf("%d",&n);
    int arr[n];
    printf("enter the integer in array:\n");
    for (i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    printf("the negative number is:\n");
     for (i=0;i<n;i++)
     {
         if (arr[i]<0)
         {
             printf("%d",arr[i]);
         }
     }
     return 0;
}
